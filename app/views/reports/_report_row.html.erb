<div class="row">
  <div class="col-md-3">
    <% if report.period.current %>
      <strong>Week of <%= report.period.start.strftime("%B %e, %Y") %></strong>
    <% else %>
      Week of <%= report.period.start.strftime("%B %e, %Y") %>
    <% end%>
  </div>
  <div class="col-md-3">
    <%= link_to 'Show', report %>
    <%= link_to 'Edit', edit_report_path(report) %>
    <%= link_to 'Destroy', report, method: :delete, data: { confirm: 'Are you sure?' } %>
  </div>
</div>